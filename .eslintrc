{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true,
      "classes": true
    },
    "root" :true
  },
  "env": {
    "node": true
  },
  "extends": [
    "defaults"
  ],
  "rules": {
    "no-console": 0,
    "no-constant-condition": 0

  }
}
